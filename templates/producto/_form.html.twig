{{ form_start(form) }}
<div class="text-center container col-lg-10 col-md-10 col-sm-10 card">
  <div class="col-md-12">
    <div class="input-group mt-12 mb-12">
      <div class="form-group col-md-5 offset-md-1">
        {{form_row(form.codigo)}}
      </div>
      <div class="form-group col-md-5">
        {{form_row(form.precioEfectivo)}}
      </div>
    </div>
    <div class="input-group mt-12 mb-12">
      <div class="form-group col-md-5 offset-md-1">
        {{form_row(form.precioCompra)}}
      </div>
      <div class="form-group col-md-5">
        {{form_row(form.precioTarjeta)}}
      </div>
    </div>
    <div class="input-group mt-12 mb-12">
    <div class="form-group col-md-2">
      {{form_row(form.estado)}}
    </div>
    <div class="form-group col-md-4">
      {{form_row(form.stock)}}
    </div>
    <div class="form-group col-md-5">
      {{form_row(form.codigoBarra)}}
    </div>
    <div class="input-group mt-12 mb-12">
      <div class="form-group col-md-5 offset-md-1">
        {{form_row(form.marca)}}
      </div>
      <div class="form-group col-md-5">
        {{form_row(form.descripcion)}}
      </div>
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
  </div>
</div>
{{ form_end(form) }}
